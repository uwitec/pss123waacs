<h1>Listing shipping_addresses</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Customer code</th>
    <th>Zip code</th>
    <th>Address</th>
    <th>Tel</th>
    <th>Fax</th>
    <th>Email</th>
    <th>Is fixed</th>
    <th>Picking style</th>
    <th>Ware house</th>
  </tr>

<% @shipping_addresses.each do |shipping_address| %>
  <tr>
    <td><%=h shipping_address.name %></td>
    <td><%=h shipping_address.customer_code %></td>
    <td><%=h shipping_address.zip_code %></td>
    <td><%=h shipping_address.address %></td>
    <td><%=h shipping_address.tel %></td>
    <td><%=h shipping_address.fax %></td>
    <td><%=h shipping_address.email %></td>
    <td><%=h shipping_address.is_fixed %></td>
    <td><%=h shipping_address.picking_style %></td>
    <td><%=h shipping_address.ware_house_id %></td>
    <td><%= link_to 'Show', shipping_address %></td>
    <td><%= link_to 'Edit', edit_shipping_address_path(shipping_address) %></td>
    <td><%= link_to 'Destroy', shipping_address, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New shipping_address', new_shipping_address_path %>