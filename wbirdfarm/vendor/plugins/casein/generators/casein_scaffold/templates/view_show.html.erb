<!-- Scaffolding generated by Casein <%= casein_get_full_version_string %> -->

<h2>View <%= singular_name.humanize.downcase %></h2>

<%% form_for :<%= file_name %>, @<%= file_name %>, :url => casein_<%= file_name %>_path, :html => {:method => :put}  do |form| %>

	<%%= render :partial => 'form', :locals => {:form => form, :<%= singular_name %> => @<%= singular_name %>} %>
	
	<p class="submits">
		<a href="<%%= url_for casein_<%= plural_name %>_path %>" class="cancel">Cancel</a> or
		<%%= form.submit "Save changes", :class => "update" %>
	</p>

<%% end %>