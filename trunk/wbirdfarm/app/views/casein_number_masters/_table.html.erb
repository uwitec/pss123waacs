<!-- Scaffolding generated by Casein v.2.0.6 -->

<table cellpadding="0" cellspacing="0" id="itemList" class="itemList">
	<tr>
		<th><%= t('label.number_master.code') %></th>
		<th><%= t('label.number_master.name') %></th>
		<th><%= t('label.number_master.last_num') %></th>
		<th><%= t('label.number_master.surfix') %></th>
		<th><%= t('label.number_master.max_num') %></th>
		<th><%= t('label.number_master.min_num') %></th>
		<th>&nbsp;</th>
	</tr>
	
	<% row = 1 %>

	<% for number_master in number_masters %>
		<tr class="<%= casein_table_row_class_name row %> hover">
			<td><%= casein_table_cell_link number_master.code, number_master %></td>
			<td><%= casein_table_cell_link number_master.name, number_master %></td>
			<td class="num"><%= casein_table_cell_link number_master.last_num, number_master %></td>
			<td class="num"><%= casein_table_cell_link number_master.surfix, number_master %></td>
			<td class="num"><%= casein_table_cell_link number_master.max_num, number_master %></td>
			<td class="num"><%= casein_table_cell_link number_master.min_num, number_master %></td>
			<td class="delete">
				<!-- %= link_to(casein_show_row_icon("delete"), casein_number_master_path(number_master), :method => :delete, :confirm => "Are you sure you want to delete this number master?") % -->
			</td>
		</tr>
		<% row += 1 %>
	<% end %>
</table>
