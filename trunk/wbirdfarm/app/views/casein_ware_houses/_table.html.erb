<!-- Scaffolding generated by Casein v.2.0.6 -->

<table cellpadding="0" cellspacing="0" id="itemList" class="itemList">
	<tr>
		<th><%= t 'label.ware_house.name' %></th>
		<th><%= t 'label.ware_house.code' %></th>
		<th><%= t 'label.ware_house.allocate_priority' %></th>
		<th><%= t 'label.ware_house.region' %></th>
		<th>&nbsp;</th>
	</tr>
	
	<% row = 1 %>

	<% for ware_house in ware_houses %>
		<tr class="<%= casein_table_row_class_name row %> hover">
			<td><%= casein_table_cell_link ware_house.name, ware_house %></td>
			<td><%= casein_table_cell_link ware_house.code, ware_house %></td>
			<td><%= casein_table_cell_link ware_house.allocate_priority, ware_house %></td>
			<td><%= casein_table_cell_link ware_house.show_region_name, ware_house %></td>
			<td class="delete">
				<%= link_to(casein_show_row_icon("delete"), casein_ware_house_path(ware_house), :method => :delete, :confirm => "Are you sure you want to delete this ware house?") %>
			</td>
		</tr>
		<% row += 1 %>
	<% end %>
</table>
