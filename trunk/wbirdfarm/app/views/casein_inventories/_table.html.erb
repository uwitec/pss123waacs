<!-- Scaffolding generated by Casein v.2.0.6 -->

<table cellpadding="0" cellspacing="0" id="itemList" class="itemList">
	<tr>
		<th>Ware house</th>
		<th>Location</th>
		<th>Allocate priority</th>
		<th>Goods code</th>
		<th>Goods name</th>
		<th>Qty</th>
		<th>Ordered qty</th>
		<th>Allocated qty</th>
		<th>Picking style</th>
		<th>&nbsp;</th>
	</tr>
	
	<% row = 1 %>

	<% for inventory in inventories %>
		<tr class="<%= casein_table_row_class_name row %> hover">
			<td><%= casein_table_cell_link inventory.show_ware_house_name, inventory %></td>
			<td><%= casein_table_cell_link inventory.location, inventory %></td>
			<td><%= casein_table_cell_link inventory.allocate_priority, inventory %></td>
			<td><%= casein_table_cell_link inventory.goods_code, inventory %></td>
			<td><%= casein_table_cell_link inventory.goods_name, inventory %></td>
			<td><%= casein_table_cell_link inventory.qty, inventory %></td>
			<td><%= casein_table_cell_link inventory.ordered_qty, inventory %></td>
			<td><%= casein_table_cell_link inventory.allocated_qty, inventory %></td>
			<td><%= casein_table_cell_link inventory.picking_style, inventory %></td>
			<td class="delete">
				<%= link_to(casein_show_row_icon("delete"), casein_inventory_path(inventory), :method => :delete, :confirm => "Are you sure you want to delete this inventory?") %>
			</td>
		</tr>
		<% row += 1 %>
	<% end %>
</table>
