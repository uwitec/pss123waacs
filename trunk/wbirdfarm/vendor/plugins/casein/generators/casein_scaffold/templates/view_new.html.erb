<!-- Scaffolding generated by Casein <%= casein_get_full_version_string %> -->

<h2>Add a new <%= singular_name.humanize.downcase %></h2>

<%% form_for :<%= file_name %>, @<%= file_name %>, :url => casein_<%= plural_name %>_path do |form| %>
	
	<%%= render :partial => 'form', :locals => {:form => form, :<%= singular_name %> => @<%= singular_name %>} %>
	
	<p class="submits">
		<a href="<%%= url_for casein_<%= plural_name %>_path %>" class="cancel">Cancel</a> or 
		<%%= form.submit "Add new <%= singular_name.humanize.downcase %>", :class => "update" %>
	</p>

<%% end %>